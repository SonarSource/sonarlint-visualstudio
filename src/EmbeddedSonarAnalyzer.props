<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- Manually set the version of the SonarC# and VB analyzers to embed.
       NB we assume that the SonarC# and SonarVB NuGet packages have the same version number. -->
  <PropertyGroup>
    <!-- Note: Guide on how to update the analyzers is on the xtranet! -->
    <EmbeddedSonarAnalyzerVersion>10.15.0.120848</EmbeddedSonarAnalyzerVersion>
    <EmbeddedSonarCFamilyAnalyzerVersion>6.72.0.89435</EmbeddedSonarCFamilyAnalyzerVersion>
    <EmbeddedSonarJSAnalyzerVersion>11.4.1.34873</EmbeddedSonarJSAnalyzerVersion>
    <EmbeddedSonarHtmlAnalyzerVersion>3.19.0.5695</EmbeddedSonarHtmlAnalyzerVersion>
    <EmbeddedSonarSecretsJarVersion>2.29.0.8107</EmbeddedSonarSecretsJarVersion>
    <EmbeddedSonarSqvsRoslynJarVersion>1.0.0.85</EmbeddedSonarSqvsRoslynJarVersion>
    <!-- SLOOP: Binaries for SonarLint Out Of Process -->
    <EmbeddedSloopVersion>10.33.0.82404</EmbeddedSloopVersion>
    <EmbeddedEsLintBridgeVersion>1.0.0</EmbeddedEsLintBridgeVersion>
  </PropertyGroup>
</Project>
