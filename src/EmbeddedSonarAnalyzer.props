<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- Manually set the version of the SonarC# and VB analyzers to embed.
       NB we assume that the SonarC# and SonarVB NuGet packages have the same version number. -->
  <PropertyGroup>
    <!-- Note: Guide on how to update the analyzers is on the xtranet! -->
    <EmbeddedSonarAnalyzerVersion>10.15.0.120848</EmbeddedSonarAnalyzerVersion>
    <EmbeddedSonarCFamilyAnalyzerVersion>6.74.0.91793</EmbeddedSonarCFamilyAnalyzerVersion>
    <EmbeddedSonarJSAnalyzerVersion>11.6.0.36606</EmbeddedSonarJSAnalyzerVersion>
    <EmbeddedSonarHtmlAnalyzerVersion>3.20.0.6289</EmbeddedSonarHtmlAnalyzerVersion>
    <EmbeddedSonarSecretsJarVersion>2.33.0.9826</EmbeddedSonarSecretsJarVersion>
    <EmbeddedSonarSqvsRoslynJarVersion>1.0.0.169</EmbeddedSonarSqvsRoslynJarVersion>
    <!-- SLOOP: Binaries for SonarLint Out Of Process -->
    <EmbeddedSloopVersion>10.36.0.83732</EmbeddedSloopVersion>
    <EmbeddedEsLintBridgeVersion>1.0.0</EmbeddedEsLintBridgeVersion>
  </PropertyGroup>
</Project>
