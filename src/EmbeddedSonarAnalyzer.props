<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <!-- Manually set the version of the SonarC# and VB analyzers to embed.
       NB we assume that the SonarC# and SonarVB NuGet packages have the same version number. -->
  <PropertyGroup>
    <!-- Note: don't forget to upload the new packages to the package feed when changing 
         the SonarAnalyzer version -->
    <EmbeddedSonarAnalyzerVersion>8.32.0.39516</EmbeddedSonarAnalyzerVersion>
    <EmbeddedSonarCFamilyAnalyzerVersion>6.27.0.38122</EmbeddedSonarCFamilyAnalyzerVersion>
    <EmbeddedSonarJSAnalyzerVersion>8.1.0.15788</EmbeddedSonarJSAnalyzerVersion>
    <EmbeddedSonarSecretsAnalyzerVersion>0.0.6</EmbeddedSonarSecretsAnalyzerVersion>
  </PropertyGroup>
 
</Project>