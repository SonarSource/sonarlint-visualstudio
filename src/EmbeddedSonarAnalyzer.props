<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- Manually set the version of the SonarC# and VB analyzers to embed.
       NB we assume that the SonarC# and SonarVB NuGet packages have the same version number. -->
  <PropertyGroup>
    <!-- Note: Guide on how to update the analyzers is on the xtranet! -->
    <!-- sonar-csharp-enterprise-plugin & sonar-vbnet-enterprise-plugin -->
    <EmbeddedSonarAnalyzerVersion>10.19.0.132793</EmbeddedSonarAnalyzerVersion>
    <!-- sonar-cfamily-plugin -->
    <EmbeddedSonarCFamilyAnalyzerVersion>6.78.0.96395</EmbeddedSonarCFamilyAnalyzerVersion>
    <!-- sonar-javascript-plugin -->
    <EmbeddedSonarJSAnalyzerVersion>11.8.0.37897</EmbeddedSonarJSAnalyzerVersion>
    <EmbeddedEsLintBridgeVersion>1.0.0</EmbeddedEsLintBridgeVersion>
    <!-- sonar-html-plugin -->
    <EmbeddedSonarHtmlAnalyzerVersion>3.22.0.7011</EmbeddedSonarHtmlAnalyzerVersion>
    <!-- sonar-text-plugin -->
    <EmbeddedSonarSecretsJarVersion>2.38.0.10279</EmbeddedSonarSecretsJarVersion>
    <!-- sonarqube-ide-visualstudio-roslyn --> 
    <EmbeddedSonarSqvsRoslynJarVersion>1.2.0.364</EmbeddedSonarSqvsRoslynJarVersion>
    <!-- SLOOP: Binaries for SonarLint Out Of Process -->
    <EmbeddedSloopVersion>10.44.0.84275</EmbeddedSloopVersion>
  </PropertyGroup>
</Project>
